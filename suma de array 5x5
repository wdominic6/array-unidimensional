#include<iostream>
using namespace std;
int main(){
    int matriz[5][5];
    int n = 1,x;
    for(int i = 0 ; i < 5 ; i++ ){
        for(int j = 0 ; j < 5 ; j++ ){
                matriz [i][j] = n;
                n++;
                x += n;
        }
    }
    matriz[2][2] = x;
    for(int i = 0 ; i < 5 ; i++){
        for(int j = 0 ; j < 5 ; j++){
            cout << matriz[i][j] << "\t";
        }
        cout << endl;
    }
}
