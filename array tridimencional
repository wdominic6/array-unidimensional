#include <iostream>
using namespace std;
int main() {
    int matriz[3][3][3];
    int n;
    for(int y = 0 ; y < 3 ; y++){
        for(int x = 0 ; x < 3 ; x++){
            for(int z = 0 ; z < 3 ; z++){
                matriz[x][y][z] = n;
                if(n==1){
                    n=0;
                }
                else{
                    n++;
                }
                cout << matriz[x][y][z] << " ";
            }
            cout << endl;
        }
    }
    return 0;
}
