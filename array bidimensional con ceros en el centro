#include <iostream>
using namespace std;
int main() {
    int matriz[5][5];
    int n = 1;
    for(int i = 0 ; i < 5 ; i++){
        for(int j = 0 ; j < 5 ; j++){
            matriz[i][j] = n;
            n++;
        }
    }
    for(int i = 1 ; i < 4 ; i++){
        for(int j = 1 ; j < 4 ; j++){
            matriz[i][j] = 0;
        }
    }
    for(int i = 0 ; i < 5 ; i++){
        for(int j = 0 ; j < 5 ; j++){
            cout << matriz[i][j] << "\t";
        }
        cout << endl;
    }
}
